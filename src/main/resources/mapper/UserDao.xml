<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.plei.springboot.mybatis.dao.UserDao">
    <insert id="save" parameterType="com.plei.springboot.mysql.entity.User">
        INSERT INTO user (create_date,name)
        VALUES (
          #{createDate},
          #{name}
        )
    </insert>
</mapper>