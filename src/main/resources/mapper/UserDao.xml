<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.plei.springboot.mybatis.dao.UserDao">
    <select id="getById" resultType="com.plei.springboot.mybatis.entity.User" parameterType="long">
        select id,name,create_date from user WHERE id=#{value}
    </select>
</mapper>